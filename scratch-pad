(def number1 [8 7 8 3 1 8 8 1 6 7])
(def number2 [3 1 4 10 8 1 3 10 10 9])
(defn pairs
  [number1 number2]
  {:number1 number1
   :number2 number2})

(def pairm (map pairs number1 number2))

;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;user/pairm</span>","value":"#'user/pairm"}
;; <=

;; @@
(def archive ())
;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;user/archive</span>","value":"#'user/archive"}
;; <=

;; @@
(defn build-archive
  [pairm archive]
(if (= 0 (- (count
              (filter #(>= (:number2 %) (:number2 (nth pairm 5))) 
              (filter #(>= (:number1 %) (:number1 (nth pairm 5))) pairm))) 1))
 (cons (nth pairm 5) archive) (cons () archive)))
;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;user/build-archive</span>","value":"#'user/build-archive"}
;; <=

;; @@
(build-archive pairm archive)
;; @@
;; =>
;;; {"type":"list-like","open":"<span class='clj-list'>(<span>","close":"<span class='clj-list'>)</span>","separator":" ","items":[{"type":"list-like","open":"<span class='clj-list'>(<span>","close":"<span class='clj-list'>)</span>","separator":" ","items":[],"value":"()"}],"value":"(())"}
