;; @@
(defn build-archive
  [pairm archive]
  (let [counter 0]
(take 5 (iterate (if (= 0 (- (count
              (filter #(>= (:number2 %) (:number2 (nth counter))) 
              (filter #(>= (:number1 %) (:number1 (nth counter))) pairm))) 1))
 (cons (nth pairm counter) archive) (cons () archive))))))
;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;user/build-archive</span>","value":"#'user/build-archive"}
;; <=

;; @@
(defn build-archive
  [pairm archive]
  (let [counter 9]
    (filter #(>= (:number1 %) (:number1 (nth counter))) pairm)))
;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;user/build-archive</span>","value":"#'user/build-archive"}
;; <=

;; @@
(defn build-archive
  [pairm archive counter]
(if (= 0 (- (count
              (filter #(>= (:number2 %) (:number2 (nth pairm counter))) 
              (filter #(>= (:number1 %) (:number1 (nth pairm counter))) pairm))) 1))
 (cons (nth pairm counter) archive) (cons () archive)))
;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;user/build-archive</span>","value":"#'user/build-archive"}
;; <=

;; @@
(def archive (build-archive pairm archive 0))
